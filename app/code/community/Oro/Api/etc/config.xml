<config>
    <modules>
        <Oro_Api>
            <version>1.0.0</version>
        </Oro_Api>
    </modules>
    <global>
        <models>
            <oro_api>
                <class>Oro_Api_Model</class>
                <resourceModel>oro_api_resource</resourceModel>
            </oro_api>
            <oro_api_resource>
                <class>Oro_Api_Model_Resource</class>
            </oro_api_resource>
            <catalog>
                <rewrite>
                    <product_api_v2>Oro_Api_Model_Catalog_Product_Api_V2</product_api_v2>
                </rewrite>
            </catalog>
        </models>
        <helpers>
            <oro_api>
                <class>Oro_Api_Helper</class>
            </oro_api>
        </helpers>
        <resources>
            <oro_api_setup>
                <setup>
                    <module>Oro_Api</module>
                </setup>
            </oro_api_setup>
        </resources>
    </global>
</config>